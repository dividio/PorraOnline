<?xml version="1.0" encoding="UTF-8"?>

<faces-config
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
    version="2.0">

<lifecycle>
	<phase-listener>com.aap.listener.FaseListener</phase-listener>
</lifecycle>

<navigation-rule>
	<display-name>General</display-name>
	<from-view-id>*</from-view-id>
	<navigation-case>
		<from-outcome>menuPrincipal</from-outcome>
		<to-view-id>/jsf/MenuPrincipal.xhtml</to-view-id>
		<redirect/>
	</navigation-case>
	<navigation-case>
        <from-outcome>listaPartidas</from-outcome>
        <to-view-id>/jsf/partidas/ListaPartidas.xhtml</to-view-id>
        <redirect/>
    </navigation-case>
    <navigation-case>
        <from-outcome>crearPartida</from-outcome>
        <to-view-id>/jsf/partidas/CrearPartida.xhtml</to-view-id>
        <redirect/>
    </navigation-case>
    <navigation-case>
        <from-outcome>administrarPartida</from-outcome>
        <to-view-id>/jsf/partidas/AdministrarPartida.xhtml</to-view-id>
        <redirect/>
    </navigation-case>
    <navigation-case>
        <from-outcome>administrarResultados</from-outcome>
        <to-view-id>/jsf/partidas/AdministrarResultados.xhtml</to-view-id>
        <redirect/>
    </navigation-case>
    <navigation-case>
        <from-outcome>mensajesPartida</from-outcome>
        <to-view-id>/jsf/partidas/MensajesPartida.xhtml</to-view-id>
        <redirect/>
    </navigation-case>
    <navigation-case>
        <from-outcome>clasificacionPartida</from-outcome>
        <to-view-id>/jsf/partidas/ClasificacionPartida.xhtml</to-view-id>
        <redirect/>
    </navigation-case>
    <navigation-case>
        <from-outcome>proximoEvento</from-outcome>
        <to-view-id>/jsf/partidas/ProximoEvento.xhtml</to-view-id>
        <redirect/>
    </navigation-case>
</navigation-rule>

</faces-config>
